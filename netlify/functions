import fetch from "node-fetch"

// get the JSON details for a youtube search result 
exports.handler = async (event, context, callback) => {
    const youtube_endpoint = 'https://youtube.googleapis.com/youtube/v3/search?part=snippet&maxResults=25&q=live%20public%20webcam&key=${process.env.YT_API_KEY}'
